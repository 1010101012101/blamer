        "xml2js": "~0.4.x"
    },
    "devDependencies": {
        "mocha": "~1.20.x",
        "chai": "~1.9.x",
        "sinon": "~1.10.x",
        "sinon-chai": "~2.5.x",
        "proxyquire": "~1.0.x"
